                                typeMissmatch="handleTypeMissmatch"
                                style="Emphasized"
                                fileType="txt,jpg,pdf,png"
                                placeholder="Choose a file to upload...">
                                <u:parameters>
                                    <u:FileUploaderParameter name="param1" value="value1"/>
                                </u:parameters>
                            </u:FileUploader>
                            <Table id="uploadedFilesTable" items="{/uploadedFiles}" class="sapUiSmallMarginTop">
                                <headerToolbar>
                                    <Toolbar>
                                        <Title text="Attachment ({/uploadedFilesCount})"/>
                                    </Toolbar>
                                </headerToolbar>
